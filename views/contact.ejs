<%- include("partials/header.ejs") %>
<h1 class="my-3">Let's connect with us!</h1>
<p class="my-3"><%= contactContent %></p>
<section class="contact-section">
    <div class="info-wrap w-100 p-lg-5 p-4 img">
        <div class="dbox w-100 d-flex align-items-start">
            <div class="icon d-flex align-items-center justify-content-center m-3">
                <i class="bi bi-geo-alt"></i>
            </div>
            <div class="text pl-3 m-3">
                <p><span class="me-1">Address:</span> Anywhere St, Any City, ST 12345</p>
            </div>
        </div>
        <div class="dbox w-100 d-flex align-items-start">
            <div class="icon d-flex align-items-center justify-content-center m-3">
                <i class="bi bi-telephone"></i>
            </div>
            <div class="text pl-3 m-3">
                <p><span class="me-1">Phone:</span> <a href="tel://1234567920">+123-456-78-90</a></p>
            </div>
        </div>
        <div class="dbox w-100 d-flex align-items-start">
            <div class="icon d-flex align-items-center justify-content-center m-3">
                <i class="bi bi-envelope"></i>
            </div>
            <div class="text pl-3 m-3">
                <p><span class="me-1">Email:</span> <a href="mailto:info@yoursite.com">foodieblog@gmail.com</a></p>
            </div>
        </div>
        <div class="dbox w-100 d-flex align-items-start">
            <div class="icon d-flex align-items-center justify-content-center m-3">
                <i class="bi bi-globe"></i>
            </div>
            <div class="text pl-3 m-3">
                <p><span class="me-1">Website:</span> <a href="#">foodieblog.com</a></p>
            </div>
        </div>
    </div>
